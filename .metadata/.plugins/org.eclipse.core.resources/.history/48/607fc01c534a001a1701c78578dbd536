package lv.anna.inputoutput;

import java.io.File;

public class TestFile {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		File myFile = new File("Data.txt");
		if(myFile.exists()){
			System.out.println(myFile.getName() + "exists");
		}else{ 
			System.out.println("File not found");
		}

	}

}
