
public class PrimType {

	
	public static void main(String[] args) {
		//Numeric data primitive type
		int integer = 2; 
		long longInteger = 20204040240L;
		short shortInteger = 2020;
		byte byteInteger = 120;
		// Floating - Point type real numbers
		double doubleFloatNumber = 0.5d;
		float floatNumber = 0.12412412f;
		// Characters 
		char charSymbol = 'a';
		char charSymbolAsNumber = 37;
		System.out.println("Char: "+charSymbolAsNumber);
		char charA = 65;
		char charN = 78;
		char charP = 80;
		char charS = 83;
		char charT = 84;
		char charR = 82;
		char charE = 69;
		System.out.println("Char:"+charA+charN+charN+charA+" "+charP+charA+charS+charT+charA+charR+charE);
		//Booleans - only 2 values true or false
		boolean booleanTrueFalse = false; 
		System.out.println(booleanTrueFalse);
		
		

	}

}
